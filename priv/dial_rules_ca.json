{
    "country": "ca",
    "trunk_prefix": "1",
    "international_prefix": "011",
    "normalized_format": "%C%a%n",
    "dial_rules":
    [
        {
            "billing_tags": ["local"],
            "regexp": "^[2-9][0-9]{6}$",
            "display_name": "Local numbers"
        },
        {
            "billing_tags": ["domestic"],
            "regexp": "^[1]{0,1}(403|587|780|236|250|604|778|204|431|506|709|902|226|249|289|343|365|416|437|519|613|647|705|807|905|418|438|450|514|579|581|819|873|306|639|867)[2-9][0-9]{6}$",
            "display_name": "NANPA Canada"
        },
        {
            "billing_tags": ["toll_free"],
            "regexp": "^18(00|11|22|33|44|55|66|77|88|99)[2-9][0-9]{6}$",
            "display_name": "Toll-free numbers"
        },
        {
            "billing_tags": ["international"],
            "regexp": "^[1]{0,1}(242|246|264|268|297|340|345|441|473|590|592|596|649|664|670|671|684|758|764|767|784|787|809|869|876|939)[2-9][0-9]{6}$",
            "display_name": "NANPA Caribbean"
        },
        {
            "billing_tags": ["international"],
            "regexp": "^[1]{0,1}[2-9][0-9]{2}[2-9][0-9]{6}$",
            "display_name": "NANPA USA"
        },
        {
            "billing_tags": ["emergency"],
            "regexp": "^[2-9]11$",
            "display_name": "Emergency numbers"
        },
        {
            "billing_tags": ["international", "premium"],
            "regexp": "^011(423)[0-9]+$",
            "display_name": "International premium numbers (Liechtenstein)"
        },
        {
            "billing_tags": ["international"],
            "regexp": "^011[2-9][0-9]+$",
            "display_name": "International numbers"
        },
        {
            "billing_tags": ["domestic", "collect"],
            "regexp": "^0[2-9][0-9]{2}[2-9][0-9]{6}$",
            "display_name": "Domestic phone numbers with dialing prefix (collect call)"
        },
        {
            "billing_tags": ["premium"],
            "regexp": "^1900[2-9][0-9]{6}$",
            "display_name": "Premium numbers"
        },
        {
            "billing_tags": ["operator"],
            "regexp": "^0$",
            "display_name": "Operator"
        },
        {
            "billing_tags": ["operator", "international"],
            "regexp": "^00$",
            "display_name": "International operator"
        }
    ]
}
